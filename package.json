{
	"name": "pxnx-discord-bot-js",
	"module": "index.ts",
	"type": "module",
	"scripts": {
		"dev": "bun run --watch index.ts",
		"start": "bun run index.ts",
		"sync": "bun run deploy-commands.ts",
		"purge": "bun run reset-commands.ts",
		"fetch": "bun run fetch-commands.ts",
		"lint": "bunx biome check .",
		"lint:fix": "bunx biome check --write .",
		"format": "bunx biome format --write .",
		"typecheck": "tsc --noEmit",
		"test": "bun test",
		"test:watch": "bun test --watch"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/bun": "latest",
		"@types/node": "^22.18.6"
	},
	"peerDependencies": {
		"typescript": "^5.6.3"
	},
	"dependencies": {
		"@discordjs/opus": "^0.10.0",
		"@discordjs/voice": "^0.19.0",
		"@distube/ytdl-core": "^4.16.12",
		"@distube/ytsr": "^2.0.4",
		"discord.js": "^14.22.1",
		"ffmpeg-static": "^5.2.0",
		"sodium-native": "^4.3.3"
	}
}
